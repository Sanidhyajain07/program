#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n,i ;
	
	cin>>n;
	
	int array[n];
	for(i=0;i<n;i++)
	{
	    cin>>array[i];
	}
    int mx, mn;
	mx =INT_MIN;
    mn=INT_MAX;
    for(i=0;i<n;i++)
    {	
    	if(array[i]>mx)
    		mx=array[i];
    	if(array[i]<mn)
    		mn=array[i];
    }
    cout<<mn<<"\n"<<mx;


	return 0;
}
